{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "AdmissionPortal": "Information"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:8000"
      }
    }
  },
  "ConnectionStrings": {
    "Redis": "localhost:6379",
    "DefaultConnection": "Server=localhost;Database=master;Trusted_Connection=True;TrustServerCertificate=True;"
  },
  "AllowedHosts": "*",
  "JwtOptions": {
    "Issuer": "Issuer",
    "Audience": "Audience",
    "Secret": "ExtremelyPowerfullSecret.Hasagi.ExtremelyPowerfullSecret",
    "TokenLifetimeMinutes": 10,
    "RefreshTokenLifetimeDays": 14
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:3000",
      "https://myfrontend.com"
    ]
  }
}
